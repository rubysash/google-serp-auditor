# https://console.cloud.google.com/apis/credentials/key/

# Flask Configuration
SECRET_KEY=your-super-secret-flask-key-here-change-this
FLASK_ENV=development

# Your Google Maps JavaScript API key
GOOGLE_MAPS_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Flask session secret key (used for cookies, OAuth sessions)
SECRET_KEY=your-super-secret-session-key

# Database Configuration
DATABASE_URL=sqlite:///data/maps_intel.db

# API Rate Limiting
MAX_REQUESTS_PER_HOUR=1000
KNOWLEDGE_GRAPH_REQUESTS_PER_DAY=10000

# Development Settings
DEBUG=True
TESTING=False

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/maps_intel.log
